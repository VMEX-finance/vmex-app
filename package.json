{
    "name": "vmex-app",
    "author": "VMEX Finance",
    "description": "React frontend for the VMEX Finance protocol enabling secure crypto lending and borrowing.",
    "version": "1.0.0",
    "private": true,
    "homepage": "https://app.vmex.finance",
    "license": "MIT",
    "keywords": [
        "vmex",
        "finance",
        "defi",
        "app"
    ],
    "bugs": {
        "url": "https://github.com/vmexfinance/vmex-app/issues",
        "email": "issues@vmex.finance"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VMEX-finance/vmex-app"
    },
    "engines": {
        "node": ">=18.13.0"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "dev": "REACT_APP_TEST=1 GENERATE_SOURCEMAP=false BROWSER=none react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "dependencies": {
        "@apollo/client": "^3.7.2",
        "@emotion/react": "11.10.5",
        "@emotion/styled": "11.10.5",
        "@headlessui/react": "^1.6.6",
        "@mui/icons-material": "5.10.9",
        "@mui/material": "^5.10.11",
        "@rainbow-me/rainbowkit": "^0.8.0",
        "@reduxjs/toolkit": "^1.8.3",
        "@tanstack/react-query": "^4.16.1",
        "@tanstack/react-query-devtools": "^4.22.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@vmexfinance/sdk": "^1.3.7",
        "@wagmi/core": "0.8.11",
        "bad-words": "^3.0.4",
        "decimal.js": "^10.4.3",
        "ethers": "^5.6.9",
        "graphql": "^16.6.0",
        "lodash.merge": "^4.6.2",
        "merkletreejs": "^0.3.10",
        "moment": "^2.29.4",
        "mui-datatables": "^4.2.2",
        "react": "^18.2.0",
        "react-app-rewired": "^2.2.1",
        "react-dom": "^18.2.0",
        "react-ga": "^3.3.1",
        "react-icons": "^4.4.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "react-simple-captcha": "^9.0.2",
        "react-slick": "^0.29.0",
        "react-toastify": "^9.1.1",
        "react-tooltip": "^5.21.1",
        "react-use": "^17.4.0",
        "recharts": "^2.1.13",
        "slick-carousel": "^1.8.1",
        "tailwindcss": "^3.1.6",
        "typescript": "^4.8.4",
        "wagmi": "0.10.5"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/jest": "^29.5.4",
        "@types/lodash.merge": "^4.6.7",
        "@types/mui-datatables": "^4.3.5",
        "@types/node": "^20.5.3",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@types/react-slick": "^0.23.10",
        "@types/react-tooltip": "^4.2.4",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "babel-plugin-named-exports-order": "^0.0.2",
        "customize-cra": "^1.0.0",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^7.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --check --write"
        ]
    }
}
